{
	"name": "space-arena",
	"version": "0.1.0",
	"description": "Space Arena game",
	"private": true,
	"repository": "https://github.com/AntonovSergey2211/space-arena.git",
	"author": "Sergey Antonov <superflasher22@gmail.com>",
	"license": "MIT",
	"scripts": {
		"clean": "rimraf ./dist",
		"watch": "tsc -w & rollup -c rollup.config.dev.js -w & yarn serve",
		"build": "yarn clean && tsc && rollup -c rollup.config.prod.js",
		"serve": "http-server ./dist/build",
		"lint": "eslint --fix",
		"compress": "yarn google-closure-compiler --compilation_level=ADVANCED --js=dist/build/bundle.js --js_output_file=dist/build/bundle.min.js"
	},
	"devDependencies": {
		"@rollup/plugin-html": "^0.2.3",
		"@typescript-eslint/eslint-plugin": "^4.29.3",
		"@typescript-eslint/parser": "^4.29.3",
		"eslint": "^7.32.0",
		"eslint-config-airbnb-base": "^14.2.1",
		"eslint-plugin-import": "^2.24.1",
		"google-closure-compiler": "^20210808.0.0",
		"http-server": "^13.0.1",
		"rimraf": "^3.0.2",
		"rollup": "^2.56.3",
		"rollup-plugin-filesize": "^9.1.1",
		"rollup-plugin-sourcemaps": "^0.6.3",
		"rollup-plugin-terser": "^7.0.2",
		"typescript": "^4.3.5"
	}
}
